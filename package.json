{
  "name": "lacherada",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:types": "node scripts/type-check.js",
    "lint:full": "npm run lint:check && npm run lint:types",
    "fix": "npm run lint && npm run format",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "node scripts/type-check.js",
    "type-check:watch": "tsc --noEmit --watch",
    "check-file": "node scripts/check-file.js",
    "check": "npm run check-file",
    "check-focused": "node scripts/check-file-focused.js",
    "quality": "npm run lint:check && npm run format:check && npm run type-check"
  },
  "dependencies": {
    "dayjs": "^1.11.15",
    "firebase": "^12.2.1",
    "fuse.js": "^7.1.0",
    "pinia": "^2.1.7",
    "primeicons": "^6.0.1",
    "primevue": "^3.46.0",
    "vue": "^3.4.0",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/language-server": "^0.14.26",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/language-server": "^3.0.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.5.0",
    "vite": "^5.0.8",
    "vue-tsc": "^2.2.12"
  }
}
